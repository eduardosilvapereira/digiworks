{
  "short_name": "digiworks",
  "name": "DigiWorks",
  "description": "Registra as entradas de tempo do Toggl.com via * registros de trabalho * para as Demandas correspondentes no Jira",
  "version": "2.0.0",
  "manifest_version": 2,
  "permissions": [
    "storage",
    "tabs",
    "activeTab",
    "webRequest",
    "webRequestBlocking",
    "<all_urls>",
    "https://toggl.com/*/*", "https://atlassian.net/*/*"
  ],
  "icons": {
    "16": "logo16.png",
    "32": "logo32.png",
    "48": "logo48.png",
    "128": "logo128.png"
  },
  "browser_action": {
    "default_title": "DigiWorks",
    "default_popup": "index.html"
  }
}
